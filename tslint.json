{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-a11y"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "only-arrow-functions": false,
    "no-console": false,
    "jsx-no-bind": false,
    "jsx-no-lambda": false,
    "member-access": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/*"
    ]
  }
}
